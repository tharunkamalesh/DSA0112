#include <iostream>
using namespace std;

int main() {
    int num, temp, rev = 0, rem;

    cout << "Enter a number: ";
    cin >> num;

    temp = num;   // store original number

    while (num != 0) {
        rem = num % 10;
        rev = rev * 10 + rem;
        num = num / 10;
    }

    if (temp == rev)
        cout << "Palindrome number";
    else
        cout << "Not a palindrome number";

    return 0;
}
